{
    "name": "awcodes/tiptap-demo",
    "type": "project",
    "description": "Demo and Testing app for Tiptap Editor Filament Plugin.",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "awcodes/filament-curator": ">=3",
        "awcodes/filament-tiptap-editor": ">=3",
        "filament/filament": "^3.2.139",
        "filament/spatie-laravel-translatable-plugin": "^3.2.139",
        "guzzlehttp/guzzle": "^7.9.2",
        "laravel/framework": "^10.48.28",
        "laravel/sanctum": "^3.3.3",
        "laravel/tinker": "^2.10.1",
        "pboivin/filament-peek": "2.x-dev"
    },
    "require-dev": {
        "awcodes/html-faker": "^0.1.0",
        "fakerphp/faker": "^1.24.1",
        "laravel/pint": "^1.20",
        "laravel/sail": "^1.41.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^7.11",
        "phpunit/phpunit": "^10.5.45",
        "spatie/laravel-ignition": "^2.9",
        "spatie/laravel-ray": "^1.39.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "~/Dev/packages/filament/tiptap-editor"
        },
        {
            "type": "path",
            "url": "~/Dev/packages/filament/curator"
        }
    ]
}
